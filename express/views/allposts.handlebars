<div class="container">
    <ul class="list-group my-2">
        {{#each blogs}}
        <a href="/blog/{{this.slug}}" class="list-group-item d-flex justify-content-between align-items-start">
            <div class="ms-2 me-auto">
                <div class="fw-bold">{{this.title}}</div>
                {{this.content}} - {{this.slug}}
            </div>

        </a>
        {{/each}}
    </ul>
</div>